{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "hotel": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::hotel.hotel",
      "inversedBy": "reviews"
    },
    "Rating": {
      "type": "decimal"
    },
    "comment": {
      "type": "text"
    },
    "review_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "approved ",
        "rejected"
      ]
    },
    "date": {
      "type": "datetime"
    },
    "username": {
      "type": "string"
    },
    "phone_number": {
      "type": "biginteger"
    }
  }
}
